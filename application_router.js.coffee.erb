#= require controllers/contacts_controller
#= require layouts/app

jQuery(document).ready ($) ->

  class ApplicationRouter extends Backbone.Router

    routes:
      ''      : '_index'

    _index: (type) ->
      @_taskView = new Do.Views.TaskList
        collection: @_taskList
        type: type

    initialize: (opts) ->
      @_taskList = new Do.Models.TaskList()